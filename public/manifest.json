{
  "short_name": "Netvyne",
  "name": "Netvyne",
  "version": "0.0",
  "manifest_version": 2,
  "key": "mlhdicffmjdikmmbnpfagiiohadjhgif",
  "background": {
    "scripts": ["app/background.js"]
  },
  "browser_action": {},
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "content_scripts" : [
    {
      "matches": [ "<all_urls>" ],
      "css": ["/css/root.css"],
      "js": ["/static/js/content.js"]
    }
  ],
  "icons": { "512": "logo.png"},
  "web_accessible_resources":[
    "/static/css/content.css", "/static/media/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
