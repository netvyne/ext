{
  "manifest_version": 2,

  "name": "Netvyne",
  "version": "0.0",

  "browser_action": {},

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "background": {
    "scripts": ["js/vendor.js", "js/background.js"]
  },

  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}