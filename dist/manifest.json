{
    "manifest_version": 2,
    "name": "Netvyne Extension",
    "description": "A Chrome Extension starter kit",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon-16.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/backgroundPage.js"],
        "persistent": true
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": ["tabs", "activeTab", "notifications", "http://*/", "https://*/", "storage", "<all_urls>", "cookies", "permissions", "webRequest", "ws://*/*", "wss://*/*"]
}
